{
  "name": "my-app",
  "main": "dist/src/main.js",
  "scripts": {
    "start": "node dist/src/main.js",
    "dev": "cross-env NODE_ENV=development nodemon --ext js,ts,json,env --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/main.ts",
    "prepare": "husky install",
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf ./dist",
    "test": "jest --coverage",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --colors --coverage --ci",
    "lint": "eslint --ext .ts,.js .",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "mongoose": "^8.5.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jest/globals": "^28.0.0",
    "@jest/types": "^28.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.19.44",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chai": "^5.1.1",
    "chai-http": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.0",
    "mocha": "^10.7.3",
    "mockingoose": "^2.16.2",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "sinon": "^18.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
